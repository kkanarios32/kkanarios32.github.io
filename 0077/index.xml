            <fr:title text="Optimization on the Riemmanian Manifold">Optimization on the Riemmanian Manifold</fr:title>
          </fr:frontmatter>
          <fr:mainmatter>
            <fr:tree show-metadata="false">
              <fr:frontmatter>
                <fr:authors />
                <fr:date>
                  <fr:year>2025</fr:year>
                  <fr:month>4</fr:month>
